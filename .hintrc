{
  "extends": [
    "development"
  ],
  "hints": {
    "no-inline-styles": "off",
    "axe/text-alternatives": [
      "default",
      {
        "image-alt": "off",
        "frame-title": "off"
      }
    ],
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "video[playsinline]",
          "iframe[referrerpolicy=no-referrer-when-downgrade]"
        ]
      }
    ],
    "axe/name-role-value": [
      "default",
      {
        "link-name": "off"
      }
    ],
    "typescript-config/consistent-casing": "off",
    "typescript-config/strict": "off",
    "axe/forms": [
      "default",
      {
        "label": "off"
      }
    ],
    "axe/aria": [
      "default",
      {
        "aria-valid-attr-value": "off"
      }
    ]
  }
}